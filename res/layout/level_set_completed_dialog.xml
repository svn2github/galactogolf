<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:orientation="vertical" android:layout_margin="10px" android:padding="10px">
	<TextView android:id="@+id/TextView01" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="@string/level_set_completed_label"
		android:textSize="20pt"></TextView>
		<TableLayout android:id="@+id/level_set_completed_score_table" android:layout_width="fill_parent" android:layout_height="wrap_content"
	android:background="#ffffff"
	>
	</TableLayout>
	<LinearLayout android:id="@+id/LinearLayout02"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:orientation="horizontal">
		<TextView android:id="@+id/TextView03" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="@string/level_set_score_total_label"
			android:textSize="10pt"></TextView>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/level_set_score_total"
			android:textSize="10pt" android:text="&lt;n&gt;"></TextView>
	</LinearLayout>

	<Button android:layout_height="wrap_content"
		android:id="@+id/level_set_completed_ok_button" android:text="Ok"
		android:layout_width="wrap_content"  android:background="@drawable/selector" android:layout_gravity="center"></Button>

</LinearLayout>
